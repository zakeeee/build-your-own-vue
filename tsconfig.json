{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "ESNext"
    ],
    "removeComments": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "strict": true,
    "baseUrl": ".",
    "rootDir": "./src",
    "outDir": "./dist",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
